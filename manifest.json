{
    "manifest_version": 3,
    "name": "SecureOps",
    "version": "1.0",
    "description": "Password protect access to any website",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Website Password Lock"
    },
    "web_accessible_resources": [
      {
        "resources": ["locked_page.html"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  