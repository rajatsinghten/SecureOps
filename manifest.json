{
  "manifest_version": 3,
  "name": "Website Locker",
  "version": "1.0",
  "permissions": [
    "storage",
    "webNavigation",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["registration.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
