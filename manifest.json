{
    "manifest_version": 2,
    "name": "SecureOps",
    "version": "1.0",
    "description": "Blocks access to specified websites",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Website Blocker"
    }
  }
  